	rdfs:domain :VehicleEngineJobCard;
	rdfs:range :Vehicle;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Vehicle engine job card has vehicle".

:vehicleHasVehicleEngineJobCard a owl:ObjectProperty, owl:InverseFunctionalProperty;
	rdfs:subPropertyOf :vehicleHasVehicleJobCard;
	owl:inverseOf :vehicleEngineJobCardHasVehicle;
	rdfs:domain :Vehicle;
	rdfs:range :VehicleEngineJobCard;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Vehicle has vehicle engine job card".

:vehiclePumpJobCardHasVehicle a owl:ObjectProperty;
	rdfs:subPropertyOf :vehicleJobCardHasVehicle;
	rdfs:domain :VehiclePumpJobCard;
	rdfs:range :Vehicle;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Vehicle pump job card has vehicle".

:vehicleHasVehiclePumpJobCard a owl:ObjectProperty, owl:InverseFunctionalProperty;
	rdfs:subPropertyOf :vehicleHasVehicleJobCard;
	owl:inverseOf :vehiclePumpJobCardHasVehicle;
	rdfs:domain :Vehicle;
	rdfs:range :VehiclePumpJobCard;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Vehicle has vehicle pump job card".

:specialRouteHasDriverWageSpecialRouteRate a owl:ObjectProperty;
	rdfs:domain :SpecialRoute;
	rdfs:range :DriverWageSpecialRouteRate;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Special route has driver wage special route rate".

:driverWageSpecialRouteRateHasSpecialRoute a owl:ObjectProperty;
	owl:inverseOf :specialRouteHasOrigin;
	rdfs:domain :DriverWageSpecialRouteRate;
	rdfs:range :SpecialRoute;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Driver wage special route rate has special route".



:vehicleNumberPlate a owl:DatatypeProperty;
	rdfs:domain :Vehicle;
	rdfs:range xsd:string;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Vehicle number plate".

:fuelLevelConversionRatio a owl:DatatypeProperty;
